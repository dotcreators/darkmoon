{
	"include": [
	  "src"
	],
	"compilerOptions": {
	  "alwaysStrict": true,
	  "noImplicitAny": true,
	  "noImplicitThis": true,
	  "strict": true,
	  "noFallthroughCasesInSwitch": true,
	  "noImplicitReturns": true,
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "module": "CommonJS",
	  "esModuleInterop": true,
	  "moduleResolution": "Node",
	  "lib": ["ESNext", "WebWorker"],
	  "target": "ESNext",
	  "sourceMap": true,
	  "declaration": true,
	  "declarationMap": true,
	  "outDir": "build",
	  "types": ["@cloudflare/workers-types"]
	}
  }